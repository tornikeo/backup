/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    Or(a=notA, b=b, out=orB);
    Or(a=a, b=notB, out=orA);
    Nand(a=orB, b=orA, out=out);
}