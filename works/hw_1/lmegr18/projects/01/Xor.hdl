/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=a, b=b, out=orAb);
    Nand(a=a, b=b, out=nandAb);
    And(a=orAb, b=nandAb, out=out);
}