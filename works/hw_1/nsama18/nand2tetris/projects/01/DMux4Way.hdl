/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    // Put your code here:
    DMux(in=in, sel=sel[0], a=tempFirst, b=tempSecond);
    Mux(a=tempFirst, b=false, sel=sel[1], out=a);
    Mux(a=tempSecond, b=false, sel=sel[1], out=b);
    Mux(a=false, b=tempFirst, sel=sel[1], out=c);
    Mux(a=false, b=tempSecond, sel=sel[1], out=d);
}