/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=a, b=a, out=notA);
    Nand(a=b, b=b, out=notB);
    Nand(a=a, b=notB, out=first);
    Nand(a=b, b=notA, out=second);
    Nand(a=first, b=second, out=out);
}